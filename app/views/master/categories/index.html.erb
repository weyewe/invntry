<h1>Listing master_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @master_categories.each do |master_category| %>
  <tr>
    <td><%= master_category.name %></td>
    <td><%= link_to 'Show', master_category %></td>
    <td><%= link_to 'Edit', edit_master_category_path(master_category) %></td>
    <td><%= link_to 'Destroy', master_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Category', new_master_category_path %>
